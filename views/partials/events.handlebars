                        <div class="evnt_addres">
                           <p>Hotel Radisson</p>
                           <p>4th Street,DOHS MIRPUR</p>
                           <p>Saturday, 20 july 2015</p>
                           <p>4:30PM - 6:15PM</p>
                        </div>
                     </div>
                     <!--.card .back .evnt_prt_back .evnt_prt_back_1-->
                  </div>
                  <!-- .cards-->

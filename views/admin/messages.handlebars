<table class="dashboard-table">
  <colgroup>
    <col width="150">
    <col width="80">
    <col width="200">
    <col width="60">
    <col width="220">
    <col width="170">
  </colgroup>
  <thead>
    <tr>
      <th><a href="#">Name <i class="fa fa-caret-down"></i></a></th>
      <th><a href="#">Email <i class="fa fa-caret-down"></i></a></th>
      <th><a href="#">Number of Guests <i class="fa fa-caret-down"></i></a></th>
      <th><a href="#">Events <i class="fa fa-caret-down"></i></a></th>
      <th><a href="#">Attending <i class="fa fa-caret-down"></i></a></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
    {{#each messages}}
      <tr class="your messages">
        <td>
          <div class="flex-container align-justify align-top">
            <div class="flex-child-grow">
              <h6 class="dashboard-table-text">{{fullname}}</h6>
              {{!-- <span class="dashboard-table-timestamp">03/04/2017</span> --}}
            </div>
          </div>
        </td>
        <td>{{email}}</td>
        <td class="bold">{{numguest}}</td>
        <td>{{allevents}}</td>
        <td>{{attending}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

